        "    edge_attr=EDGE_BASE,\n",
        ")\n",
        "\n",
        "# ---- Swimlane: Data ----\n",
        "with m.subgraph(name=\"cluster_data\") as c:\n",
        "    c.attr(label=\"Data\", **CLUSTER)\n",
        "    c.node(\"D1\", \"FIM Construct\\n18 items (1‚Äì7) in 6 domains\\nTotal: 18‚Äì126\")\n",
        "    c.node(\"D2\", \"Row Assembly\\n‚Ä¢ total, previous, Œî (derive if missing)\\n‚Ä¢ motor, cognitive\\n‚Ä¢ ref_text (ground truth)\")\n",
        "    c.edge(\"D1\",\"D2\")\n",
        "\n",
        "# ---- Swimlane: Splits & Selection ----\n",
        "with m.subgraph(name=\"cluster_split\") as c:\n",
        "    c.attr(label=\"Splits & Selection\", **CLUSTER)\n",
        "    c.node(\"S1\", \"Splits\\n‚Ä¢ 80/20 TEST by patient\\n‚Ä¢ TRAIN ‚Üí 5-fold GroupKFold\")\n",
        "    c.node(\"S2\", \"FS Example Selection (per target row)\\n‚Ä¢ K nearest by numeric fields\\n‚Ä¢ TRAIN-fold only\\n‚Ä¢ exclude same patient & report\")\n",
        "    c.edge(\"S1\",\"S2\")\n",
        "\n",
        "# ---- Swimlane: Prompting ----\n",
        "with m.subgraph(name=\"cluster_prompt\") as c:\n",
        "    c.attr(label=\"Prompting\", **CLUSTER)\n",
        "    c.node(\"P1\", \"Zero-Shot (ZS)\\nSYSTEM: clinical neurorehab expert\\nUSER: facts block only\")\n",
        "    c.node(\"P2\", \"Few-Shot (FS, style-only)\\nHeader enforces: style only\\nExamples = (facts + ref_text)\\nQuery = new facts only\")\n",
        "    # link split->prompt to show FS depends on selection\n",
        "    m.edge(\"S2\",\"P2\")\n",
        "    m.edge(\"S1\",\"P1\", style=\"dotted\")\n",
        "\n",
        "# ---- Swimlane: Inference ----\n",
        "with m.subgraph(name=\"cluster_infer\") as c:\n",
        "    c.attr(label=\"Inference\", **CLUSTER)\n",
        "    c.node(\"I1\", \"Gemini Generation\\nstateless API, per row\")\n",
        "    c.edge(\"P1\",\"I1\")\n",
        "    c.edge(\"P2\",\"I1\")\n",
        "\n",
        "# ---- Swimlane: Post-processing ----\n",
        "with m.subgraph(name=\"cluster_post\") as c:\n",
        "    c.attr(label=\"Post-processing\", **CLUSTER)\n",
        "    c.node(\"Q1\", \"Normalize Output\\n‚Ä¢ strip dates (dd.mm.yyyy)\\n‚Ä¢ ‚â§2 sentences\\n‚Ä¢ remove list bullets\")\n",
        "    c.edge(\"I1\",\"Q1\")\n",
        "\n",
        "# ---- Swimlane: Evaluation ----\n",
        "with m.subgraph(name=\"cluster_eval\") as c:\n",
        "    c.attr(label=\"Evaluation\", **CLUSTER)\n",
        "    c.node(\"E1\", \"Numeric Fidelity\\n‚Ä¢ exact total / previous / Œî\\n‚Ä¢ numbers_ok_core (‚â•2 of 3)\\n‚Ä¢ extraneous_ok (no spurious ints)\")\n",
        "    c.node(\"E2\", \"Consistency\\n‚Ä¢ direction_ok (lexical + numeric)\")\n",
        "    c.node(\"E3\", \"Similarity\\n‚Ä¢ TF-IDF cosine (masked numbers/dates)\\n‚Ä¢ BERTScore F1 (semantic)\")\n",
        "    c.node(\"E4\", \"Composite\\n‚Ä¢ ClinDocScore = 0.50*core + 0.20*extr + 0.15*TFIDF + 0.15*BERT\")\n",
        "    c.edge(\"Q1\",\"E1\"); c.edge(\"E1\",\"E2\"); c.edge(\"E2\",\"E3\"); c.edge(\"E3\",\"E4\")\n",
        "\n",
        "# ---- Swimlane: Analysis & Reporting ----\n",
        "with m.subgraph(name=\"cluster_stats\") as c:\n",
        "    c.attr(label=\"Analysis & Reporting\", **CLUSTER)\n",
        "    c.node(\"A1\", \"Paired Tests\\n‚Ä¢ McNemar (binary)\\n‚Ä¢ Wilcoxon (paired)\\n‚Ä¢ Cliff‚Äôs Œ¥, bootstrap CI\")\n",
        "    c.node(\"A2\", \"Outputs\\n‚Ä¢ CV/TEST tables\\n‚Ä¢ figures & discussion\")\n",
        "    c.edge(\"E4\",\"A1\"); c.edge(\"A1\",\"A2\")\n",
        "\n",
