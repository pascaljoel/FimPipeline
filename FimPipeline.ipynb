      "cell_type": "code",
      "source": [
        "# ================== Cleaner pipeline diagrams (TRAIN-CV vs TEST lanes) ==================\n",
        "# Installs + imports\n",
        "!apt-get -qq update && apt-get -qq install -y graphviz >/dev/null\n",
        "!pip -q install graphviz\n",
        "from graphviz import Digraph\n",
        "from IPython.display import SVG, display\n",
        "\n",
        "# Helper + style\n",
        "def render_graph(dot: Digraph, filename: str, fmt: str = \"svg\"):\n",
        "    dot.format = fmt\n",
        "    path = dot.render(filename, cleanup=True)\n",
        "    if fmt == \"svg\":\n",
        "        display(SVG(path))\n",
        "    print(f\"Saved {fmt.upper()}: {path}\")\n",
        "\n",
        "NODE_BASE = {\n",
        "    \"shape\": \"box\",\n",
        "    \"style\": \"rounded,filled\",\n",
        "    \"fillcolor\": \"#F6F8FA\",\n",
        "    \"color\": \"#9AA0A6\",\n",
        "    \"fontsize\": \"11\",\n",
        "    \"fontname\": \"Calibri\",\n",
        "}\n",
        "EDGE_BASE = {\"arrowhead\": \"normal\", \"color\": \"#6E6E6E\"}\n",
        "CLUSTER   = {\"color\": \"#DADCE0\", \"style\": \"rounded\", \"fontname\": \"Calibri\"}\n",
        "\n",
        "# ------------------ HIGH-LEVEL ROADMAP (compact, L‚ÜíR) ------------------\n",
        "hl = Digraph(\"overview_clean\",\n",
        "             graph_attr={\"rankdir\":\"LR\",\"splines\":\"spline\",\"nodesep\":\"0.7\",\"ranksep\":\"1.0\",\"fontname\":\"Calibri\"},\n",
        "             node_attr=NODE_BASE, edge_attr=EDGE_BASE)\n",
        "\n",
        "with hl.subgraph(name=\"cluster_inputs\") as c:\n",
        "    c.attr(label=\"Inputs\", **CLUSTER)\n",
        "    c.node(\"IN\", \"Data schema\\nFIM total, previous, Œî, domains\\nref_text (ground truth)\\nIDs (patient/report)\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_splits\") as c:\n",
        "    c.attr(label=\"Splits\", **CLUSTER)\n",
        "    c.node(\"SP\", \"By patient\\n80/20 TEST (hold-out)\\nTRAIN ‚Üí 5-fold GroupKFold\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_prompt\") as c:\n",
        "    c.attr(label=\"Prompting\", **CLUSTER)\n",
        "    c.node(\"PR\", \"ZS: system + facts\\nFS: style-only K examples\\n(no content leakage)\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_gen\") as c:\n",
        "    c.attr(label=\"Generation\", **CLUSTER)\n",
        "    c.node(\"GE\", \"Gemini (stateless)\\nper row\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_post\") as c:\n",
        "    c.attr(label=\"Post-processing\", **CLUSTER)\n",
        "    c.node(\"PO\", \"strip dates\\n‚â§2 sentences\\nno lists\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_eval\") as c:\n",
        "    c.attr(label=\"Evaluation\", **CLUSTER)\n",
        "    c.node(\"EV\", \"numeric: exact/extra/Œî dir\\nsimilarity: TF-IDF, BERT\\ncomposite: ClinDocScore\")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_stats\") as c:\n",
        "    c.attr(label=\"Statistics & Output\", **CLUSTER)\n",
        "    c.node(\"ST\", \"paired tests, CIs\\nper-fold means\\nCV/TEST tables, figures\")\n",
        "\n",
        "hl.edges([(\"IN\",\"SP\"),(\"SP\",\"PR\"),(\"PR\",\"GE\"),(\"GE\",\"PO\"),(\"PO\",\"EV\"),(\"EV\",\"ST\")])\n",
        "\n",
        "# Leakage note\n",
        "hl.node(\"LG\", \"Leakage guards:\\nFS examples from TRAIN-fold only;\\nexclude same patient/report\",\n",
        "        shape=\"note\", fillcolor=\"#FFF9C4\", color=\"#C9AE34\", fontsize=\"10\")\n",
        "hl.edge(\"SP\",\"LG\", style=\"dashed\"); hl.edge(\"LG\",\"PR\", style=\"dashed\")\n",
        "\n",
        "render_graph(hl, \"diagram_overview_clean\", fmt=\"svg\")\n",
        "render_graph(hl, \"diagram_overview_clean\", fmt=\"png\")\n",
        "\n",
        "# ------------------ DETAILED METHODOLOGY (two lanes: TRAIN-CV vs TEST) ------------------\n",
        "m = Digraph(\"methodology_clean\",\n",
        "            graph_attr={\"rankdir\":\"LR\",\"splines\":\"spline\",\"nodesep\":\"0.6\",\"ranksep\":\"1.1\",\"fontname\":\"Calibri\"},\n",
        "            node_attr=NODE_BASE, edge_attr=EDGE_BASE)\n",
        "\n",
        "# Phase 0: Inputs & Splits\n",
        "with m.subgraph(name=\"cluster_0\") as c:\n",
        "    c.attr(label=\"Inputs & Splits\", **CLUSTER)\n",
        "    c.node(\"D\", \"Data schema\\nFIM fields + ref_text + IDs\")\n",
        "    c.node(\"S\", \"Splitting (by patient)\\n80/20 TEST; TRAIN‚Üí5-fold CV\")\n",
        "    c.edge(\"D\",\"S\")\n",
        "\n",
        "# TRAIN lane (Cross-Validation)\n",
        "with m.subgraph(name=\"cluster_train\") as c:\n",
        "    c.attr(label=\"TRAIN (Cross-Validation)\", **CLUSTER)\n",
        "    c.node(\"T1\", \"Fold k (GroupKFold)\\ntrain_k / val_k\")\n",
        "    c.node(\"T2\", \"FS example selection\\nK nearest by numeric\\nwithin train_k only\")\n",
        "    c.node(\"T3\", \"Prompt\\nZS: system+facts\\nFS: style-only + stressed query\")\n",
        "    c.node(\"T4\", \"Generation\\nGemini per row\")\n",
        "    c.node(\"T5\", \"Post-proc\\nstrip dates, ‚â§2 sentences\")\n",
        "    c.node(\"T6\", \"Evaluation (val_k)\\nnumeric ‚Ä¢ similarity ‚Ä¢ composite\")\n",
        "    c.node(\"T7\", \"Aggregate across folds\\nper-fold means, variability\")\n",
        "\n",
        "# TEST lane (Hold-out)\n",
        "with m.subgraph(name=\"cluster_test\") as c:\n",
        "    c.attr(label=\"TEST (Hold-out)\", **CLUSTER)\n",
        "    c.node(\"E1\", \"TEST set (20%)\\nby patient\")\n",
        "    c.node(\"E2\", \"FS example bank\\n= full TRAIN\")\n",
        "    c.node(\"E3\", \"Prompt & Generation\\n(as in TRAIN)\")\n",
        "    c.node(\"E4\", \"Post-proc\\n(as in TRAIN)\")\n",
        "    c.node(\"E5\", \"Evaluation (TEST)\\nnumeric ‚Ä¢ similarity ‚Ä¢ composite\")\n",
        "    c.node(\"E6\", \"Final tables & figures\")\n",
        "\n",
        "# Flow wiring\n",
        "m.edge(\"S\",\"T1\"); m.edge(\"T1\",\"T2\"); m.edge(\"T2\",\"T3\"); m.edge(\"T3\",\"T4\"); m.edge(\"T4\",\"T5\"); m.edge(\"T5\",\"T6\"); m.edge(\"T6\",\"T7\")\n",
        "m.edge(\"S\",\"E1\"); m.edge(\"E1\",\"E2\"); m.edge(\"E2\",\"E3\"); m.edge(\"E3\",\"E4\"); m.edge(\"E4\",\"E5\"); m.edge(\"E5\",\"E6\")\n",
        "\n",
        "# Callouts\n",
        "m.node(\"G1\", \"Leakage guards:\\nFS uses only train_k (CV)\\nExclude same patient/report\",\n",
        "       shape=\"note\", fillcolor=\"#FFF9C4\", color=\"#C9AE34\", fontsize=\"10\")\n",
        "m.edge(\"T1\",\"G1\", style=\"dashed\"); m.edge(\"G1\",\"T2\", style=\"dashed\")\n",
        "\n",
        "m.node(\"G2\", \"Data hygiene:\\nŒî derivation, date masking,\\nwhitespace normalization\",\n",
        "       shape=\"note\", fillcolor=\"#E3F2FD\", color=\"#64B5F6\", fontsize=\"10\")\n",
        "m.edge(\"D\",\"G2\", style=\"dashed\"); m.edge(\"G2\",\"T5\", style=\"dashed\"); m.edge(\"G2\",\"E4\", style=\"dashed\")\n",
        "\n",
        "render_graph(m, \"diagram_methodology_clean\", fmt=\"svg\")\n",
        "render_graph(m, \"diagram_methodology_clean\", fmt=\"png\")\n"
      ],
