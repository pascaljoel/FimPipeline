        "# ----------------- FIGURE A: High-level roadmap (Top ‚Üí Down, single chain) -----------------\n",
        "hl = Digraph(\n",
        "    \"overview_topdown\",\n",
        "    graph_attr={\n",
        "        \"rankdir\": \"TB\",        # TOP ‚Üí DOWN\n",
        "        \"splines\": \"ortho\",     # right-angle edges to reduce crossings\n",
        "        \"nodesep\": \"0.50\",\n",
        "        \"ranksep\": \"0.70\",\n",
        "        \"fontname\": \"Calibri\"\n",
        "    },\n",
        "    node_attr=NODE_BASE,\n",
        "    edge_attr=EDGE_BASE,\n",
        ")\n",
        "\n",
        "with hl.subgraph(name=\"cluster_flow\") as c:\n",
        "    c.attr(label=\"High-level Roadmap\", **CLUSTER)\n",
        "    c.node(\"A\", \"Inputs\\nFIM total, previous, Œî, domains\\nref_text, IDs\")\n",
        "    c.node(\"B\", \"Splitting (by patient)\\n80/20 TEST hold-out\\nTRAIN ‚Üí 5-fold GroupKFold\")\n",
        "    c.node(\"C\", \"Prompting\\nZS: system + facts\\nFS: style-only K examples\")\n",
        "    c.node(\"D\", \"Generation\\nGemini (stateless) ‚Ä¢ per row\")\n",
        "    c.node(\"E\", \"Post-processing\\nstrip dates ‚Ä¢ ‚â§2 sentences ‚Ä¢ no lists\")\n",
        "    c.node(\"F\", \"Evaluation\\nnumeric (exact/extra/dir)\\nsimilarity (TF-IDF, BERT)\\ncomposite (ClinDocScore)\")\n",
        "    c.node(\"G\", \"Statistics & Output\\npaired tests, CIs ‚Ä¢ per-fold means\\nCV/TEST tables ‚Ä¢ figures\")\n",
        "\n",
        "# strictly vertical flow to avoid overlaps\n",
        "hl.edge(\"A\",\"B\")\n",
        "hl.edge(\"B\",\"C\")\n",
        "hl.edge(\"C\",\"D\")\n",
        "hl.edge(\"D\",\"E\")\n",
        "hl.edge(\"E\",\"F\")\n",
        "hl.edge(\"F\",\"G\")\n",
        "\n",
        "# Small note without extra wiring (keeps it clean)\n",
        "hl.node(\"N\", \"Leakage guard:\\nFS examples only from TRAIN-fold; exclude same patient/report\",\n",
        "        shape=\"note\", fillcolor=\"#FFF9C4\", color=\"#C9AE34\", fontsize=\"10\")\n",
        "\n",
        "render_graph(hl, \"diagram_overview_topdown\", fmt=\"svg\")\n",
        "render_graph(hl, \"diagram_overview_topdown\", fmt=\"png\")\n",
        "\n",
        "\n",
        "# ----------------- FIGURE B: Methodology (Top ‚Üí Down, two vertical lanes) -----------------\n",
        "m = Digraph(\n",
        "    \"methodology_topdown\",\n",
        "    graph_attr={\n",
        "        \"rankdir\": \"TB\",        # TOP ‚Üí DOWN\n",
        "        \"splines\": \"ortho\",     # right-angle edges\n",
        "        \"nodesep\": \"0.50\",\n",
        "        \"ranksep\": \"0.80\",\n",
        "        \"fontname\": \"Calibri\",\n",
        "        \"newrank\": \"true\"       # helps keep columns aligned\n",
        "    },\n",
        "    node_attr=NODE_BASE,\n",
        "    edge_attr=EDGE_BASE,\n",
        ")\n",
        "\n",
        "# Header (single top block)\n",
        "with m.subgraph(name=\"cluster_header\") as c:\n",
        "    c.attr(label=\"Inputs & Splitting\", **CLUSTER)\n",
        "    c.node(\"H1\", \"Data schema\\nFIM fields + ref_text + IDs\")\n",
        "    c.node(\"H2\", \"Splitting (by patient)\\n80/20 TEST hold-out ‚Ä¢ TRAIN‚Üí5-fold CV\")\n",
        "    c.edge(\"H1\",\"H2\")\n",
        "\n",
        "# --- Left column: TRAIN (Cross-Validation) ---\n",
        "with m.subgraph(name=\"cluster_train\") as tr:\n",
        "    tr.attr(label=\"TRAIN (Cross-Validation)\", **CLUSTER)\n",
        "    tr.node(\"T1\", \"Fold k (GroupKFold):\\ntrain_k / val_k\")\n",
        "    tr.node(\"T2\", \"FS example selection:\\nK nearest by numeric\\n(train_k only)\")\n",
        "    tr.node(\"T3\", \"Prompting:\\nZS: system+facts\\nFS: style-only + query facts\")\n",
        "    tr.node(\"T4\", \"Generation:\\nGemini per row\")\n",
        "    tr.node(\"T5\", \"Post-processing:\\nstrip dates ‚Ä¢ ‚â§2 sentences\")\n",
        "    tr.node(\"T6\", \"Evaluation on val_k:\\nnumeric ‚Ä¢ similarity ‚Ä¢ composite\")\n",
        "    tr.node(\"T7\", \"Aggregate across folds:\\nper-fold means ‚Ä¢ variability\")\n",
        "\n",
        "# --- Right column: TEST (Hold-out) ---\n",
        "with m.subgraph(name=\"cluster_test\") as te:\n",
        "    te.attr(label=\"TEST (Hold-out)\", **CLUSTER)\n",
        "    te.node(\"E1\", \"TEST set (20%)\")\n",
        "    te.node(\"E2\", \"FS example bank:\\nfull TRAIN\")\n",
        "    te.node(\"E3\", \"Prompt & Generation:\\n(as in TRAIN)\")\n",
        "    te.node(\"E4\", \"Post-processing:\\n(as in TRAIN)\")\n",
        "    te.node(\"E5\", \"Evaluation on TEST:\\nnumeric ‚Ä¢ similarity ‚Ä¢ composite\")\n",
        "    te.node(\"E6\", \"Final tables & figures\")\n",
        "\n",
        "# --- Routing: from Splitting to both columns (downward only) ---\n",
        "m.edge(\"H2\",\"T1\")   # into TRAIN lane\n",
        "m.edge(\"H2\",\"E1\")   # into TEST lane\n",
        "\n",
        "# TRAIN lane (strictly vertical)\n",
        "m.edge(\"T1\",\"T2\")\n",
        "m.edge(\"T2\",\"T3\")\n",
        "m.edge(\"T3\",\"T4\")\n",
        "m.edge(\"T4\",\"T5\")\n",
        "m.edge(\"T5\",\"T6\")\n",
        "m.edge(\"T6\",\"T7\")\n",
        "\n",
        "# TEST lane (strictly vertical)\n",
        "m.edge(\"E1\",\"E2\")\n",
        "m.edge(\"E2\",\"E3\")\n",
        "m.edge(\"E3\",\"E4\")\n",
        "m.edge(\"E4\",\"E5\")\n",
        "m.edge(\"E5\",\"E6\")\n",
        "\n",
        "# --- Notes (no cross wiring to keep it clean) ---\n",
        "m.node(\"GN\", \"Leakage guard:\\nFS uses only train_k in CV;\\nexclude same patient/report\",\n",
        "       shape=\"note\", fillcolor=\"#FFF9C4\", color=\"#C9AE34\", fontsize=\"10\")\n",
        "m.node(\"DH\", \"Data hygiene:\\nŒî derivation ‚Ä¢ date masking\\nwhitespace normalization\",\n",
        "       shape=\"note\", fillcolor=\"#E3F2FD\", color=\"#64B5F6\", fontsize=\"10\")\n",
        "\n",
        "render_graph(m, \"diagram_methodology_topdown\", fmt=\"svg\")\n",
        "render_graph(m, \"diagram_methodology_topdown\", fmt=\"png\")\n"
      ],
