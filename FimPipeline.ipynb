      "source": [
        "# ==== Build a Word-friendly PNG table with explanations from cv_df / test_df_out ====\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Expect cv_df and test_df_out in memory; if not, raise a friendly error.\n",
        "if \"cv_df\" not in globals() or \"test_df_out\" not in globals():\n",
        "    raise RuntimeError(\"Run your evaluation first to define cv_df and test_df_out.\")\n",
        "\n",
        "# Define metrics ‚Üí (ZS_col, FS_col, type, explanation)\n",
        "# type: \"pct\" for boolean; \"mean\" for continuous (similarities/scores)\n",
        "METRICS = [\n",
        "    (\"Exact match ‚Äî total\",    \"ZS_match_total\",      \"FS_match_total\",      \"pct\",\n",
        "     \"Output contains the correct FIM total integer.\"),\n",
        "    (\"Exact match ‚Äî previous\", \"ZS_match_previous\",   \"FS_match_previous\",   \"pct\",\n",
        "     \"Output contains the correct previous total integer.\"),\n",
        "    (\"Exact match ‚Äî Œî\",        \"ZS_match_delta\",      \"FS_match_delta\",      \"pct\",\n",
        "     \"Output contains the correct change Œî (total‚àíprevious).\"),\n",
        "    (\"Numbers OK (core)\",      \"ZS_numbers_ok_core\",  \"FS_numbers_ok_core\",  \"pct\",\n",
        "     \"‚â•2 of {total, previous, Œî} present (minimal coherence).\"),\n",
        "    (\"Extraneous OK\",          \"ZS_extraneous_ok\",    \"FS_extraneous_ok\",    \"pct\",\n",
        "     \"No integers beyond allowed set (row values + FIM constants).\"),\n",
        "    (\"Direction OK\",           \"ZS_dir_ok\",           \"FS_dir_ok\",           \"pct\",\n",
        "     \"Claimed improvement/worsening matches the numbers.\"),\n",
        "    (\"TF-IDF cosine (Œº)\",      \"ZS_TFIDF_sim_ref\",    \"FS_TFIDF_sim_ref\",    \"mean\",\n",
        "     \"Lexical similarity with numbers/dates masked.\"),\n",
