      "cell_type": "code",
      "source": [
        "# ===================== Word-ready results exports (no clipping) =====================\n",
        "# Produces:\n",
        "#   /content/results_metrics_table_pil.png   ‚Üê big, readable table (300 dpi)\n",
        "#   /content/results_significance.txt        ‚Üê Wilcoxon p-values + bootstrap CIs (CV & TEST)\n",
        "#   /content/example_triplet_pil.png         ‚Üê ref vs ZS vs FS (best illustrative TEST case)\n",
        "#\n",
        "# Optional: uncomment the download lines at the end for one-click downloads in Colab.\n",
        "\n",
        "import os, math\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "from PIL import Image, ImageDraw, ImageFont\n",
        "from scipy.stats import wilcoxon\n",
        "\n",
        "# -------------------------- Guards --------------------------\n",
        "if \"cv_df\" not in globals() or \"test_df_out\" not in globals():\n",
        "    raise RuntimeError(\"Run your evaluation first so cv_df and test_df_out exist.\")\n",
        "\n",
        "# -------------------------- Metric spec --------------------------\n",
        "METRICS = [\n",
        "    (\"Exact match ‚Äî total\",    \"ZS_match_total\",      \"FS_match_total\",      \"pct\",\n",
        "     \"Output contains the correct FIM total integer.\"),\n",
        "    (\"Exact match ‚Äî previous\", \"ZS_match_previous\",   \"FS_match_previous\",   \"pct\",\n",
        "     \"Output contains the correct previous total integer.\"),\n",
        "    (\"Exact match ‚Äî Œî\",        \"ZS_match_delta\",      \"FS_match_delta\",      \"pct\",\n",
        "     \"Output contains the correct change Œî (total‚àíprevious).\"),\n",
        "    (\"Numbers OK (core)\",      \"ZS_numbers_ok_core\",  \"FS_numbers_ok_core\",  \"pct\",\n",
        "     \"‚â•2 of {total, previous, Œî} present (minimal coherent change statement).\"),\n",
        "    (\"Extraneous OK\",          \"ZS_extraneous_ok\",    \"FS_extraneous_ok\",    \"pct\",\n",
        "     \"No integers beyond the allowed set (row values + FIM constants).\"),\n",
        "    (\"Direction OK\",           \"ZS_dir_ok\",           \"FS_dir_ok\",           \"pct\",\n",
        "     \"Claimed improvement/worsening/stability matches the numbers.\"),\n",
        "    (\"TF-IDF cosine (Œº)\",      \"ZS_TFIDF_sim_ref\",    \"FS_TFIDF_sim_ref\",    \"mean\",\n",
        "     \"Lexical similarity with numbers/dates masked.\"),\n",
        "    (\"BERTScore F1 (Œº)\",       \"ZS_BERTScore_F1\",     \"FS_BERTScore_F1\",     \"mean\",\n",
        "     \"Semantic similarity based on contextual embeddings.\"),\n",
        "    (\"ClinDocScore (Œº)\",       \"ZS_ClinDocScore\",     \"FS_ClinDocScore\",     \"mean\",\n",
        "     \"Composite: 0.50*core + 0.20*extr + 0.15*TF-IDF + 0.15*BERT.\"),\n",
        "]\n",
        "\n",
        "def _avg(series):\n",
        "    s = pd.to_numeric(pd.Series(series), errors=\"coerce\")\n",
        "    return float(np.nanmean(s)) if len(s) else np.nan\n",
        "\n",
        "def summarize_block(df, zs_col, fs_col, kind):\n",
        "    if zs_col not in df.columns or fs_col not in df.columns:\n",
        "        return (np.nan, np.nan, np.nan)\n",
        "    if kind == \"pct\":\n",
        "        zs = 100.0 * _avg(df[zs_col])\n",
        "        fs = 100.0 * _avg(df[fs_col])\n",
        "    else:\n",
        "        zs = _avg(df[zs_col]); fs = _avg(df[fs_col])\n",
        "    return zs, fs, fs - zs\n",
        "\n",
        "rows = []\n",
        "for label, zc, fc, kind, expl in METRICS:\n",
        "    cv_zs, cv_fs, cv_d = summarize_block(cv_df, zc, fc, kind)\n",
        "    te_zs, te_fs, te_d = summarize_block(test_df_out, zc, fc, kind)\n",
        "    rows.append([label, expl, kind, cv_zs, cv_fs, cv_d, te_zs, te_fs, te_d])\n",
        "\n",
        "tab = pd.DataFrame(rows, columns=[\n",
        "    \"Metric\", \"Explanation\", \"_kind\",\n",
        "    \"CV_ZS\", \"CV_FS\", \"CV_Œî(FS‚àíZS)\",\n",
        "    \"TEST_ZS\", \"TEST_FS\", \"TEST_Œî(FS‚àíZS)\"\n",
        "])\n",
        "\n",
        "def _fmt(v, kind):\n",
        "    if pd.isna(v): return \"‚Äî\"\n",
        "    return f\"{v:.1f}%\" if kind==\"pct\" else f\"{v:.3f}\"\n",
        "\n",
        "for i, kind in enumerate(tab[\"_kind\"].values):\n",
        "    for col in [\"CV_ZS\",\"CV_FS\",\"CV_Œî(FS‚àíZS)\",\"TEST_ZS\",\"TEST_FS\",\"TEST_Œî(FS‚àíZS)\"]:\n",
        "        tab.at[i, col] = _fmt(tab.at[i, col], \"pct\" if kind==\"pct\" else \"mean\")\n",
        "\n",
        "tab = tab.drop(columns=[\"_kind\"])\n",
        "\n",
        "# -------------------------- PIL table renderer (no clipping) --------------------------\n",
        "def _load_font(font_size=22):\n",
        "    # Try DejaVu Sans (present in most Colab images); fallback to default bitmap.\n",
        "    try:\n",
        "        from matplotlib import font_manager\n",
        "        fpath = font_manager.findfont(\"DejaVu Sans\")\n",
        "        return ImageFont.truetype(fpath, font_size), ImageFont.truetype(fpath, font_size)\n",
        "    except Exception:\n",
        "        f = ImageFont.load_default()\n",
        "        return f, f\n",
        "\n",
        "def _wrap_to_width(text, w_px, font, draw, pad_x, line_pad):\n",
        "    \"\"\"Wrap by measuring with the font so lines never overflow column width.\"\"\"\n",
        "    if text is None: return [\"\"]\n",
        "    text = str(text)\n",
        "    max_w = max(10, w_px - 2*pad_x)\n",
        "    lines = []\n",
        "    for para in text.split(\"\\n\"):\n",
        "        words = para.split(\" \")\n",
        "        cur = \"\"\n",
        "        for word in words:\n",
        "            cand = word if cur == \"\" else (cur + \" \" + word)\n",
        "            w = draw.textbbox((0,0), cand, font=font)[2]\n",
        "            if w <= max_w:\n",
        "                cur = cand\n",
        "            else:\n",
        "                if cur == \"\":\n",
        "                    # single long token: hard-wrap by character until it fits\n",
        "                    chunk = \"\"\n",
        "                    for ch in word:\n",
        "                        cand2 = chunk + ch\n",
        "                        if draw.textbbox((0,0), cand2, font=font)[2] <= max_w:\n",
        "                            chunk = cand2\n",
        "                        else:\n",
        "                            if chunk:\n",
        "                                lines.append(chunk)\n",
        "                            chunk = ch\n",
        "                    if chunk:\n",
        "                        lines.append(chunk)\n",
        "                    cur = \"\"\n",
        "                else:\n",
        "                    lines.append(cur)\n",
        "                    cur = word\n",
        "        if cur != \"\":\n",
        "            lines.append(cur)\n",
        "        if para == \"\" and (len(lines)==0 or lines[-1]!=\"\"):\n",
        "            lines.append(\"\")\n",
        "    return lines if lines else [\"\"]\n",
        "\n",
        "def render_table_png_pil(df: pd.DataFrame,\n",
        "                         out_path: str,\n",
        "                         widths_px=None,\n",
        "                         font_size=22,\n",
        "                         cell_pad_x=18,\n",
        "                         cell_pad_y=12,\n",
        "                         line_pad=8,\n",
        "                         header_fill=(235,235,235),\n",
        "                         grid_color=(0,0,0)):\n",
        "    if widths_px is None:\n",
        "        # Wider Explanation column; tweak if needed.\n",
        "        widths_px = [360, 980, 180, 180, 200, 180, 180, 220]\n",
        "    assert len(widths_px) == df.shape[1], \"widths_px must match number of columns\"\n",
        "\n",
        "    font, font_b = _load_font(font_size)\n",
        "    # Dummy image for text measurement\n",
        "    tmp_img = Image.new(\"RGB\", (10,10), (255,255,255))\n",
        "    draw = ImageDraw.Draw(tmp_img)\n",
        "\n",
        "    headers = list(df.columns)\n",
        "    rows = df.values.tolist()\n",
        "\n",
        "    # Wrap header and body cells\n",
        "    wrapped_header = []\n",
        "    for j, h in enumerate(headers):\n",
        "        wrapped_header.append(_wrap_to_width(str(h), widths_px[j], font_b, draw, cell_pad_x, line_pad))\n",
        "\n",
        "    wrapped = []\n",
        "    for r in rows:\n",
        "        wrapped.append([\n",
        "            _wrap_to_width(str(r[j]), widths_px[j], font, draw, cell_pad_x, line_pad)\n",
        "            for j in range(len(headers))\n",
        "        ])\n",
        "\n",
        "    def cell_height(lines, fnt):\n",
        "        ascent, descent = fnt.getmetrics()\n",
        "        lh = ascent + descent\n",
        "        return cell_pad_y*2 + len(lines)*lh + (len(lines)-1)*line_pad\n",
        "\n",
        "    header_h = max(cell_height(lines, font_b) for lines in wrapped_header)\n",
        "    row_heights = [max(cell_height(wrapped[i][j], font) for j in range(len(headers))) for i in range(len(rows))]\n",
        "\n",
        "    W = sum(widths_px) + 2\n",
        "    H = int(header_h + sum(row_heights) + 2 + (len(rows)+1))  # + grid lines\n",
        "\n",
        "    img = Image.new(\"RGB\", (W, H), (255,255,255))\n",
        "    draw = ImageDraw.Draw(img)\n",
        "\n",
        "    # Header background\n",
        "    draw.rectangle([0, 0, W-1, header_h], fill=header_fill)\n",
        "\n",
        "    # Header text\n",
        "    x = 0\n",
        "    for j, h in enumerate(headers):\n",
        "        tx = x + cell_pad_x\n",
        "        ty = cell_pad_y\n",
        "        for line in wrapped_header[j]:\n",
        "            draw.text((tx, ty), line, font=font_b, fill=(0,0,0))\n",
        "            ascent, descent = font_b.getmetrics()\n",
        "            ty += (ascent + descent + line_pad)\n",
        "        x += widths_px[j]\n",
        "\n",
        "    # Grid lines\n",
        "    draw.line([(0, header_h), (W, header_h)], fill=grid_color, width=1)\n",
        "    cx = 0\n",
        "    for w in widths_px:\n",
        "        draw.line([(cx, 0), (cx, H)], fill=grid_color, width=1)\n",
        "        cx += w\n",
        "    draw.line([(W-1, 0), (W-1, H)], fill=grid_color, width=1)\n",
        "\n",
        "    # Rows\n",
        "    y = header_h\n",
        "    for i in range(len(rows)):\n",
        "        draw.line([(0, y), (W, y)], fill=grid_color, width=1)\n",
        "        cx = 0\n",
        "        for j in range(len(headers)):\n",
        "            tx = cx + cell_pad_x\n",
        "            ty = y + cell_pad_y\n",
        "            for line in wrapped[i][j]:\n",
        "                draw.text((tx, ty), line, font=font, fill=(0,0,0))\n",
        "                ascent, descent = font.getmetrics()\n",
        "                ty += (ascent + descent + line_pad)\n",
        "            cx += widths_px[j]\n",
        "        y += row_heights[i]\n",
        "    draw.line([(0, H-1), (W, H-1)], fill=grid_color, width=1)\n",
        "\n",
        "    img.save(out_path, format=\"PNG\", optimize=True, dpi=(300,300))\n",
        "    return out_path\n",
        "\n",
        "png_path = render_table_png_pil(tab, out_path=\"results_metrics_table_pil.png\")\n",
        "print(\"Saved:\", os.path.abspath(png_path))\n",
        "\n",
        "# -------------------------- Significance (CV & TEST) --------------------------\n",
        "def paired_stats(df, metric):\n",
        "    zc, fc = f\"ZS_{metric}\", f\"FS_{metric}\"\n",
        "    if zc not in df.columns or fc not in df.columns:\n",
        "        return None\n",
        "    zs = pd.to_numeric(df[zc], errors=\"coerce\")\n",
        "    fs = pd.to_numeric(df[fc], errors=\"coerce\")\n",
        "    m = (~zs.isna()) & (~fs.isna())\n",
        "    if m.sum() < 1: return None\n",
        "    try:\n",
        "        stat, p = wilcoxon(zs[m], fs[m], zero_method=\"wilcox\", alternative=\"two-sided\", correction=False, mode=\"auto\")\n",
        "    except ValueError:\n",
        "        stat, p = (np.nan, 1.0)\n",
        "    diffs = (fs[m] - zs[m]).values\n",
        "    rng = np.random.default_rng(42)\n",
        "    boots = [float(np.mean(diffs[rng.integers(0, len(diffs), len(diffs))])) for _ in range(4000)]\n",
        "    lo, hi = float(np.quantile(boots, 0.025)), float(np.quantile(boots, 0.975))\n",
        "    return {\"n\": int(m.sum()), \"mean_diff\": float(np.mean(diffs)), \"wilcoxon_p\": float(p), \"boot_CI\": (lo, hi)}\n",
        "\n",
        "lines = []\n",
        "for split_name, df in [(\"CV\", cv_df), (\"TEST\", test_df_out)]:\n",
        "    lines.append(f\"=== {split_name} ===\")\n",
        "    for metric in [\"TFIDF_sim_ref\", \"BERTScore_F1\", \"ClinDocScore\"]:\n",
        "        res = paired_stats(df, metric)\n",
        "        if res:\n",
        "            lo, hi = res[\"boot_CI\"]\n",
        "            lines.append(f\"{metric}: n={res['n']}  Œî(FS‚àíZS)={res['mean_diff']:.3f}  95% CI=[{lo:.3f}, {hi:.3f}]  Wilcoxon p={res['wilcoxon_p']:.4f}\")\n",
        "        else:\n",
        "            lines.append(f\"{metric}: (not available)\")\n",
        "    lines.append(\"\")\n",
        "with open(\"results_significance.txt\",\"w\") as f:\n",
        "    f.write(\"\\n\".join(lines))\n",
        "print(\"\\n\".join(lines))\n",
        "print(\"Saved:\", os.path.abspath(\"results_significance.txt\"))\n",
        "\n",
        "# -------------------------- Example triplet (TEST) --------------------------\n",
        "def pick_best_example(df: pd.DataFrame):\n",
        "    df = df.copy()\n",
        "    gains = []\n",
        "    if \"FS_TFIDF_sim_ref\" in df and \"ZS_TFIDF_sim_ref\" in df:\n",
        "        df[\"gain_tfidf\"] = pd.to_numeric(df[\"FS_TFIDF_sim_ref\"], errors=\"coerce\") - pd.to_numeric(df[\"ZS_TFIDF_sim_ref\"], errors=\"coerce\")\n",
        "        gains.append(\"gain_tfidf\")\n",
        "    if \"FS_BERTScore_F1\" in df and \"ZS_BERTScore_F1\" in df:\n",
        "        df[\"gain_bertscore\"] = pd.to_numeric(df[\"FS_BERTScore_F1\"], errors=\"coerce\") - pd.to_numeric(df[\"ZS_BERTScore_F1\"], errors=\"coerce\")\n",
        "        gains.append(\"gain_bertscore\")\n",
        "    if gains:\n",
        "        df[\"gain_combo\"] = df[gains].max(axis=1, skipna=True)\n",
        "        return df.loc[df[\"gain_combo\"].idxmax()]\n",
        "    return df.iloc[0]\n",
        "\n",
        "ex = pick_best_example(test_df_out)\n",
        "rid, pid = str(ex.get(\"report_id\",\"\")), str(ex.get(\"patient_id\",\"\"))\n",
        "ref, zs, fs = str(ex.get(\"ref_text\",\"\")), str(ex.get(\"zs_text\",\"\")), str(ex.get(\"fs_text\",\"\"))\n",
        "print(f\"\\nPicked example: report_id={rid} | patient_id={pid}\")\n",
        "\n",
        "def render_triplet_png(ref, zs, fs,\n",
        "                       out_path=\"example_triplet_pil.png\",\n",
        "                       title=\"Example ‚Äî Ground truth vs ZS vs FS\",\n",
        "                       width_px=2200, font_size=22):\n",
        "    # Fonts\n",
        "    try:\n",
        "        from matplotlib import font_manager\n",
        "        fpath = font_manager.findfont(\"DejaVu Sans\")\n",
        "        font = ImageFont.truetype(fpath, font_size)\n",
        "        font_b = ImageFont.truetype(fpath, font_size)\n",
        "    except Exception:\n",
        "        font = ImageFont.load_default(); font_b = font\n",
        "\n",
        "    pad = 28\n",
        "    col1_w = int(width_px * 0.20)\n",
        "    col2_w = width_px - col1_w - 2*pad\n",
        "\n",
        "    def wrap_measured(text, w_px, font):\n",
        "        tmp = Image.new(\"RGB\", (10,10), (255,255,255))\n",
        "        drw = ImageDraw.Draw(tmp)\n",
        "        max_w = max(10, w_px - 2*pad)\n",
        "        out = []\n",
        "        for para in str(text).split(\"\\n\"):\n",
        "            words = para.split(\" \")\n",
        "            cur = \"\"\n",
        "            for word in words:\n",
        "                cand = word if cur==\"\" else (cur + \" \" + word)\n",
        "                if drw.textbbox((0,0), cand, font=font)[2] <= max_w:\n",
        "                    cur = cand\n",
        "                else:\n",
        "                    if cur == \"\":\n",
        "                        # very long token\n",
        "                        chunk = \"\"\n",
        "                        for ch in word:\n",
        "                            if drw.textbbox((0,0), chunk+ch, font=font)[2] <= max_w:\n",
        "                                chunk += ch\n",
        "                            else:\n",
        "                                if chunk: out.append(chunk)\n",
        "                                chunk = ch\n",
        "                        if chunk: out.append(chunk)\n",
        "                        cur = \"\"\n",
        "                    else:\n",
        "                        out.append(cur); cur = word\n",
        "            if cur != \"\": out.append(cur)\n",
        "            if para == \"\" and (len(out)==0 or out[-1]!=\"\"): out.append(\"\")\n",
        "        return out if out else [\"\"]\n",
        "\n",
        "    rows = [\n",
        "        (\"Ground truth (ref_text)\", ref),\n",
        "        (\"Zero-shot (ZS)\", zs),\n",
        "        (\"Few-shot (FS)\", fs),\n",
        "    ]\n",
        "    lh = font.getmetrics()[0] + font.getmetrics()[1]\n",
        "    line_pad = 10\n",
        "    header_h = lh + 2*pad\n",
        "    total_h = header_h\n",
        "    wrapped_rows = []\n",
        "    for name, text in rows:\n",
        "        name_h = lh\n",
        "        text_lines = wrap_measured(text, col2_w, font)\n",
        "        text_h = len(text_lines)*lh + (len(text_lines)-1)*line_pad\n",
        "        row_h = max(name_h, text_h) + 2*pad\n",
        "        total_h += row_h + 2\n",
        "        wrapped_rows.append((name, text_lines, row_h))\n",
        "\n",
        "    img = Image.new(\"RGB\", (width_px, total_h+pad), (255,255,255))\n",
        "    drw = ImageDraw.Draw(img)\n",
        "    # Header\n",
        "    drw.text((pad, pad//2), title, font=font_b, fill=(0,0,0))\n",
        "    y = header_h\n",
        "    x1 = pad; x2 = pad + col1_w; x3 = pad + col1_w + col2_w\n",
        "    # Vertical lines\n",
        "    drw.line([(x1, header_h), (x1, total_h)], fill=(0,0,0), width=1)\n",
        "    drw.line([(x2, header_h), (x2, total_h)], fill=(0,0,0), width=1)\n",
        "    drw.line([(x3, header_h), (x3, total_h)], fill=(0,0,0), width=1)\n",
        "\n",
        "    for (name, text_lines, row_h) in wrapped_rows:\n",
        "        drw.line([(x1, y), (x3, y)], fill=(0,0,0), width=1)\n",
        "        drw.text((x1 + pad, y + pad), name, font=font_b, fill=(0,0,0))\n",
        "        ty = y + pad\n",
        "        for line in text_lines:\n",
        "            drw.text((x2 + pad, ty), line, font=font, fill=(0,0,0))\n",
        "            ty += lh + line_pad\n",
        "        y += row_h\n",
        "    drw.line([(x1, y), (x3, y)], fill=(0,0,0), width=1)\n",
        "\n",
        "    img.save(out_path, format=\"PNG\", optimize=True, dpi=(300,300))\n",
        "    return out_path\n",
        "\n",
        "trip_path = render_triplet_png(ref, zs, fs)\n",
        "print(\"Saved:\", os.path.abspath(trip_path))\n",
