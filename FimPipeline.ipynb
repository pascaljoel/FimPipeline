      "cell_type": "code",
      "source": [
        "hl = Digraph(\n",
        "    \"high_level_roadmap\",\n",
        "    graph_attr={\"rankdir\": \"LR\", \"splines\": \"spline\", \"nodesep\": \"0.7\", \"ranksep\": \"1.0\", \"fontname\":\"Calibri\"},\n",
        "    node_attr=NODE_BASE,\n",
        "    edge_attr=EDGE_BASE,\n",
        ")\n",
        "\n",
        "# --- clusters (phases) ---\n",
        "with hl.subgraph(name=\"cluster_inputs\") as c:\n",
        "    c.attr(label=\"Inputs\", **CLUSTER)\n",
        "    c.node(\"IN\", \"Data & Requirements\\n‚Ä¢ FIM fields: total, previous, Œî, domains\\n‚Ä¢ Pseudonymized IDs\\n‚Ä¢ Ground-truth ref_text (Excel)\")\n",
        "with hl.subgraph(name=\"cluster_splits\") as c:\n",
        "    c.attr(label=\"Splits\", **CLUSTER)\n",
        "    c.node(\"SP\", \"Patient-level splitting\\n‚Ä¢ 80/20 hold-out TEST\\n‚Ä¢ TRAIN ‚Üí 5-fold GroupKFold\")\n",
        "with hl.subgraph(name=\"cluster_prompt\") as c:\n",
        "    c.attr(label=\"Prompting\", **CLUSTER)\n",
        "    c.node(\"PR\", \"ZS: system + facts block\\nFS: K style examples (TRAIN-fold only)\")\n",
        "with hl.subgraph(name=\"cluster_gen\") as c:\n",
        "    c.attr(label=\"Generation\", **CLUSTER)\n",
        "    c.node(\"GE\", \"Gemini calls per row (stateless)\\nTemp + tokens by regime\")\n",
        "with hl.subgraph(name=\"cluster_post\") as c:\n",
        "    c.attr(label=\"Post-processing\", **CLUSTER)\n",
        "    c.node(\"PO\", \"Normalize\\n‚Ä¢ strip dates\\n‚Ä¢ ‚â§2 sentences\\n‚Ä¢ no lists\")\n",
        "with hl.subgraph(name=\"cluster_eval\") as c:\n",
        "    c.attr(label=\"Evaluation\", **CLUSTER)\n",
        "    c.node(\"EV\", \"Safety + Similarity\\n‚Ä¢ exact numbers / extraneous\\n‚Ä¢ direction consistency\\n‚Ä¢ TF-IDF cosine / BERTScore\\n‚Ä¢ composite ClinDocScore\")\n",
        "with hl.subgraph(name=\"cluster_stats\") as c:\n",
        "    c.attr(label=\"Statistics\", **CLUSTER)\n",
        "    c.node(\"ST\", \"Paired tests\\n‚Ä¢ McNemar, Wilcoxon\\n‚Ä¢ Cliff‚Äôs Œ¥, bootstrap CI\\n‚Ä¢ per-fold means\")\n",
        "with hl.subgraph(name=\"cluster_out\") as c:\n",
        "    c.attr(label=\"Outputs\", **CLUSTER)\n",
        "    c.node(\"OU\", \"Tables & Figures\\n‚Ä¢ CV/TEST summaries\\n‚Ä¢ discussion, limitations\")\n",
        "\n",
        "# main flow\n",
        "hl.edge(\"IN\",\"SP\")\n",
        "hl.edge(\"SP\",\"PR\")\n",
        "hl.edge(\"PR\",\"GE\")\n",
        "hl.edge(\"GE\",\"PO\")\n",
        "hl.edge(\"PO\",\"EV\")\n",
        "hl.edge(\"EV\",\"ST\")\n",
        "hl.edge(\"ST\",\"OU\")\n",
        "\n",
        "# leakage guard note\n",
        "hl.node(\"LG\", \"Leakage Guards\\n‚Ä¢ examples = TRAIN fold only\\n‚Ä¢ exclude same patient/report\", shape=\"note\", fillcolor=\"#FFF9C4\", color=\"#C9AE34\")\n",
        "hl.edge(\"SP\",\"LG\", style=\"dashed\")\n",
        "hl.edge(\"LG\",\"PR\", style=\"dashed\")\n",
        "\n",
        "# render + save\n",
        "render_graph(hl, \"diagram_high_level\", fmt=\"svg\")\n",
        "render_graph(hl, \"diagram_high_level\", fmt=\"png\")\n"
      ],
